.de-introduction{
    margin: 40px auto 0;
    width: 1000px;
}
.de-introduction-title {
    font-size: 24px;
    color: #333;
    line-height: 28px;
}
.de-blue{
    width: 4px;
    height: 28px;
    background-color: #37aae8;
    float: left;
    margin-right: 15px;
}
.de-company{
    font-size: 20px;
    color: #333;
    line-height: 35px;
    margin-top: 25px;
    letter-spacing: 5px;
	word-wrap: break-word;
	overflow:hidden;
text-indent:2em;
}
#allmap{
    margin: 20px 0 50px;
    width: 967px;
    height: 434px;
}
.de-modal-opacity{
	 width: 1210px;
    height: 170px;
    position: absolute;
    bottom: 0;
    color: #fff;
    background-color: #000;
    opacity: 0.3;
}
