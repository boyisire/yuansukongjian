@charset "utf-8";
body {
    background-color: #f1f1f1;
}

#de-main {
    width: 1210px;
    background-color: #fff;
    margin: 20px auto 0;
}

.de-particular {
    width: 1000px;
    height: 150px;
    position: absolute;
    color: #fff;
    left: 100px;
    bottom: 0;
}

.de-SBI-name {
    font-size: 40px;
    font-weight: bold;
    float: left;
    width: 940px;
    height: 44px;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    line-height: 41px;
}

.de-iconaddress {
    background-image: url(../../../images/home/detail/address-icon.png);
    width: 16px;
    height: 26px;
    float: left;
    margin-right: 20px;
    margin-left: 20px;
}
.de-particular a{
	color:#fff !important;
}
.de-address {
    font-size: 20px;
}

.de-privilege {
    margin-top: 20px;
}

.de-privilege-tab {
    font-size: 18px;
    font-weight: bold;
    float: left;
    line-height: 30px;

}

.de-privilege-p {
    font-size: 14px;
    font-weight: bold;
    width: 350px;
    float: left;
    line-height: 30px;
}

.de-icon-privilege {
    background-image: url(../../../images/home/detail/privilege-tab-icon.png);
    width: 21px;
    height: 21px;
    float: left;
    margin-right: 12px;
}

.de-advantage {
    margin-left: 5px;
    float: left;
    font-weight: bold;
    letter-spacing: 3px;
}

.de-youhui {
    float: left;
    width: 381px;
    margin-top: 30px;
}

.de-money {
    position: absolute;
    right: 210px;
    top: 50px;
}

.de-money span {
    color: #f5894e;
    font-size: 40px;
}

.de-money p {
    margin-left: 30px;
    float: left;
    font-weight: bold;

}
.de-request{
    width: 145px;
    height: 46px;
    border-radius: 4px;
    /*background-color: #3cb1f0;*/
    background-color: #e21111;
    font-size: 24px;
    text-align: center;
    line-height: 46px;
    position: absolute;
    right: -23px;
    top: 40px;
    cursor: pointer;
    font-weight: 300;
}
.de-request-succ{
    width: 145px;
    height: 46px;
    border-radius: 4px;
