@charset "UTF-8";
body{
    background-color: #f1f1f1;
    font-family: "苹方";
}
div#home-main{
    width: 1210px;
    background-color: #fff;
    margin: 32px auto 0;
    border: 1px solid transparent;
    overflow:hidden;
}
#home-the-left{
    width: 280px;
    height: 1090px;
    border-right: 1px solid #e3e3e3;
    margin: 66px 42px 0 32px;
    float: left;
}
#home-check-in{
    background-color: #d3edff;
}
.home-the-left-change{
    width: 280px;
    height: 126px;
    border-bottom: 1px solid #e3e3e3;
    cursor: pointer;
    border-top: 1px solid transparent;
    position: relative;
}
.home-the-left-change-p1{
    font-size: 22px;
    color: #333;
    margin-top: 38px ;
    text-align: center;
}
.home-the-left-change-p2{
    font-size: 14px;
    color: #666;
    margin-top: 14px;
    text-align: center;
}
.home-gt-icon{
    width: 8px;
    height: 14px;
    background-image: url(../../images/home/gt-icon.png);
    position: absolute;
    right:25px;
    top: 60px;
}
.home-connection{
    text-align: left;
    color: #333333;
    margin-top: 30px;
    margin-left: 40px;
}
#home-the-right{
    width: 850px;
    float: right;
    margin-top: 66px;
}
.home-choose-city{

}
.home-choose-city p,.home-choose-tab>p{
    font-size: 14px;
    line-height: 30px;
    margin-right: 18px;
    float: left;
}
.home-choose-tab>p{
    margin-top: 25px;
}
select.prov,select.city,select.dist{
    width:  186px;
    height: 30px;
}
select.city,select.dist{
    margin-left: 40px;
}
.home-choose-tab{
    /*margin-top: 25px;*/
}
.home-tab{
    padding: 1px 10px;

    border: 1px solid #37aae8;
    font-size: 14px;
    font-weight: bold;
    text-align: center;
    border-radius: 15px;
    float: left;
    margin-right: 20px;
    position: relative;
    cursor: pointer;
    margin-top: 25px;
    background-color: #37aae8;
    color: #fff;
}
.home-tab-activate{
    color: #fff;
    background-color: #37aae8;
    border: 1px solid #37aae8;
}
.home-tab-img-activate{
    width: 21px;
    height: 21px;
    background-image: url(../../images/home/activate-icon.png);
    position: absolute;
    right: -11px;
    top: -11px;;
}
.home-list-detail{
    width: 800px;
    height: 184px;
    margin-top: 33px;
    /*border-top: 1px solid #e3e3e3;*/
    border-bottom: 1px solid #e3e3e3;
    overflow: hidden;
}
.home-list-detail > img{
    display: block;
    margin: 30px 18px 30px 7px;
    float: left;
    width:130px;
    height:130px;
}
.home-list-text{
    height: 130px;
    float: left;
    margin-top: 30px;
    width: 480px;
}
.home-list-name{
    font-size: 24px;
    font-weight: 900;
    color: #333;
    float: left;
    max-width: 300px;
     white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}
.home-list-icontab{
    background-image: url(../../images/home/tab-icon.png);
    width: 21px;
    height: 21px;
   margin: 10px 10px 0 8px;
    float: left;
}
.home-list-advantage{
    font-size: 14px;
    color: #666;
    font-weight: 700;
    line-height: 24px;
    float: left;
    width: 340px;
    height: 45px;
    overflow: hidden;
}
.home-list-address{
    margin-top: 12px;
    float: left;
}
.home-list-addressP{
    font-size: 18px;
    color: #666;
}
.home-list-iconaddress{
    background-image: url(../../images/home/address-icon.png);
    width: 11px;
    height: 19px;
    float: left;
    margin: 3px 8px 0 8px;
}
.home-list-address-policy{
    margin-top: 10px;
    width: 400px;
    height: 52px;
    overflow: hidden;
}
.home-list-money{
    float: right;
    margin: 60px 0px 64px 0;
}
.home-list-money span{
    color: #f5894e;
    font-size: 20px;
    font-weight: 900;
}
.home-list-money p{
    margin-bottom: 10px;
}
