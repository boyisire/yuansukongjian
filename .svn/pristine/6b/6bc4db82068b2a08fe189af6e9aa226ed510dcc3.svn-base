@charset "utf-8";
#te-main {
    width: 1210px;
    color: #ffffff;
    margin: 0 auto;
    border: 1px solid transparent;
    margin-top: 12px;
}

.te-h1 {
    color: #333;
    font-size: 26px;
    font-weight: bold;
    text-align: center;
    margin-top: 110px;
}

.te-logo-upload {
    color: #333;
    margin: 50px auto;
}

.te-a-upload {
    position: relative;
    display: inline-block;
    border-radius: 4px;
   /* padding: 40px 18px;*/
    overflow: hidden;
    text-decoration: none;
    text-indent: 0;
    width: 170px;
    height: 170px;
    /*background-image: url(../../../images/home/Form/te-logo.png);*/
    font-size: 18px;
    color: #fff;
    line-height: 30px;
    text-align: center;
    margin-right: 114px;
}

.te-a-upload input {
    position: absolute;
    font-size: 100px;
    right: 0;
    top: 0;
    opacity: 0;
}

.te-logo-upload div {
    float: left;
}

.te-logo-upload-title {
    margin-left: 255px;
    margin-right: 111px;
    line-height: 170px;
    font-size: 18px;
    width: 150px;
}

.te-lg-warn {
    width: 132px;
    height: 24px;
    /*border: 1px solid #999;*/
    border-radius: 10px;
    line-height: 24px;
    text-align: center;
    margin-top: 75px;
}

.te-room {
    color: #333;
    margin-top: 50px;
}

.te-room div {
    float: left;
}

.te-room-name {
    font-size: 18px;
    line-height: 40px;
    margin-left: 247px;
    margin-right: 40px;
    width: 150px;
    text-align: right;

}

.te-room input {
    width: 334px;
    height: 40px;
    border: 1px solid #999;
    border-radius: 5px;
}

.te-room-right {
    background-image: url(../../../images/home/Form/right.png);
    width: 21px;
    height: 21px;
    margin: 10px 170px 0 10px;
    display: none;
}

.te-room-error p {
    float: left;
    line-height: 40px;
    color: #999;
    font-weight: bold;
}

.te-image-error {
    background-image: url(../../../images/home/Form/error.png);
    width: 21px;
    height: 21px;
    margin: 10px 10px 0 10px;

}

.te-presentation {
    margin-top: 40px;
}

.te-presentation textarea {
    width: 334px;
    height: 176px;
    border: 1px solid #999;
    border-radius: 5px;
    resize: none;
}

.te-uploads {
    margin-top: 50px;
}

.status_bar {
    line-height: 44px !important;
}

.te-down-main {
    width: 1210px;
    color: #ffffff;
    margin: 0 auto;
    border: 1px solid transparent;
}

.te-preferential {
    color: #333;
    margin-top: 50px;
}

.te-preferential div {
    float: left;
}

.te-preferential textarea {
    width: 334px;
    height: 176px;
    border: 1px solid #999;
    border-radius: 5px;
    resize: none;
}

.te-room-address div {
    float: left;
}

.te-room-address {
    color: #333;
    margin-top: 50px;
}
#city select{
    width: 122px;
    height: 30px;
    margin-right: 30px;
    margin-top: 5px;
}
.te-room-detailed-add div{
    float: left;
}
.te-room-detailed-add{
    color: #333;
    margin-top: 50px;
}
.te-down-main input{
    width: 354px;
    height: 40px;
    border: 1px solid #999;
    border-radius: 4px;
    margin-left: -8px;
}
.te-tips{
    height: 28px;
    background-color: #00b8ec;
    border-radius: 14px;
    line-height: 28px;
    color: #ffffff;
    padding: 0 20px;
    margin: 0 40px 40px 0;
    cursor: pointer;
    position: relative;
}
.te-room-money div{
    float: left;
}
.te-room-money{
    font-size: 18px;
    line-height: 40px;
    margin-left: 247px;
    margin-right: 40px;
    text-align: right;
    color: #333;
    margin-top: 50px;
}
.te-money-name{
    width: 150px;
    margin-right: 40px;
}
.te-room-money input{
    width: 114px;
    height: 40px;
    border-radius: 4px;
    border: 1px solid #999;
}
.te-submit{
    width: 334px;
    height: 40px;
    background-color: #3cb1f0;
    color: #fff;
    font-size: 18px;
    border: 1px solid transparent;
    -webkit-border-radius: 4px;
    -moz-border-radius: 4px;
    border-radius: 4px;
    cursor: pointer;
    margin: 90px 0 100px 438px ;
}
.te-room-error{
    display: none;
}
.on{
    display: block !important;
}
.te-tips-p{
    background-image: url(../../../images/home/Form/right.png);
    width: 21px;
    height: 21px;
    position: absolute;
    top: -8px;
    right: -8px;
}
