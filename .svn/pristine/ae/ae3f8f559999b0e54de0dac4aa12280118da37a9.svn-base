           $('#search-main').addClass('shadow');
        }else {
            $('#menu-box').removeClass('shadow');
            $('#search-main').removeClass('shadow');
        }
    }
    $(window).scroll(BottomJumpPage);
    $('.nav-search').click(function () {
        if ($('#search-main').is(":hidden")){
            $('#search-main').show()
        }else {
            $('#search-main').hide()
        }
