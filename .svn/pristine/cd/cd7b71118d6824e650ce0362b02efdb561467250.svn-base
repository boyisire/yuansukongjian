<{include file="./tpl/Cms/Public/header.html" /}>
<body>
    <div id="wrapper">
        <{include file="./tpl/Cms/Public/left.html" /}>
            <div class="row wrapper border-bottom white-bg page-heading">
                <div class="col-lg-10">
                    <h2>空间列表</h2>
                    <ol class="breadcrumb">
                        <li>
                            <a href="index.html">空间管理</a>
                        </li>
                        <li>
                            <a>空间合作</a>
                        </li>
                        <li>
                            <strong>空间列表</strong>
                        </li>
                    </ol>
                </div>
                <div class="col-lg-2">

                </div>
            </div>
            <div class="wrapper wrapper-content animated fadeInRight">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="ibox float-e-margins">
                            <div class="ibox-title">
                                <h5>基本 <small>分类，查找</small></h5>
                                <div class="ibox-tools">
                                    <a class="collapse-link">
                                        <i class="fa fa-chevron-up"></i>
                                    </a>
                                    <a class="dropdown-toggle" data-toggle="dropdown" href="table_data_tables.html#">
                                        <i class="fa fa-wrench"></i>
                                    </a>
                                    <ul class="dropdown-menu dropdown-user">
                                        <li><a href="table_data_tables.html#">选项1</a>
                                        </li>
                                        <li><a href="table_data_tables.html#">选项2</a>
                                        </li>
                                    </ul>
                                    <a class="close-link">
                                        <i class="fa fa-times"></i>
                                    </a>
                                </div>
                            </div>
                            <div class="ibox-content">
                                <div>
                                    <table class="table">
                                        <tr>
                                            <td style="margin-left:0px;width:10px"><a href="__ROOT__/Cms/InnoCustomer/index">
                                                <button class="btn btn-success m-r-sm" type="button">全部 【<--{$cntNum.all}-->】</button>
                                            </a></td>
                                            <td style="margin-left:-100px;width:10px"><a href="__ROOT__/Cms/InnoCustomer/index/is_pass/1">
                                            <button class="btn btn-warning m-r-sm" type="button">已处理 【<--{$cntNum.use}-->】</button>
                                            </a></td>
                                            <td ><a href="__ROOT__/Cms/InnoCustomer/index/is_pass/0"><button class="btn btn-danger m-r-sm" type="button">未处理 【<--{$cntNum.unuse}-->】</button>
                                            </a></td>
                                        </tr>
                                    </table>
                                </div>
                                <table class="table table-striped table-bordered table-hover dataTables-example">
                                    <thead>
                                        <tr>
                                            <th><input type="radio"></th>
                                            <th>ID</th>
                                            <th>企业名称</th>
                                            <th>企业描述</th>
                                            <th>地理位置</th>
                                            <th>所属行业</th>
                                            <th>融资阶段</th>
                                            <th>负责人</th>
                                            <th>电话</th>
                                            <th>产品链接</th>
                                            <th>企划书</th>
                                            <th>通过状态</th>
                                            <th>是否联系</th>
                                            <th>创建时间</th>
                                            <th>操作</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <{foreach name="list" item="vo" key="k"}>
                                        <tr>
                                            <td><input type="radio"></td>
                                            <td><--{$vo.id}--></td>
                                            <td><--{$vo.name}--></td>
                                            <td><--{$vo.desc|msubstr=0,15,'utf-8',true}--></td>
                                            <td><--{$vo.prov}-->.<--{$vo.city}-->.<--{$vo.dist}--></td>
                                            <td><--{$vo.industry}--></td>
                                            <td><--{$vo.financing}--></td>
                                            <td><--{$vo.principal}--></td>
                                            <td><--{$vo.mobile}--></td>
                                            <td><a href="<--{$vo.product}-->" target="_blank">
                                                <button>查看产品</button>
                                            </a></td>
                                            <!--企划书-->
                                            <td>
                                            <{if condition="$vo.prospectus eq 'bp@yuansuzhouqi.cn'"}>
                                                <a href="Mailto:<--{$vo.prospectus}-->" title="<--{$vo.prospectus}-->">
                                                    <button>查收邮件</button>
                                                </a>
                                            <{else /}>
                                                <a href="__URL__/downLoad/id/<--{$vo.id}-->" title="<--{$vo.prospectus}-->">
                                                    <button>下载企划书</button>
                                                </a>
                                            <{/if}>
                                            </td>
                                            <td>
                                                <{if condition="$vo['is_pass'] == 0"}>
                                                    <font color="#3cb1f0">申请中</font>
                                                    <a href="__ROOT__/Cms/InnoCustomer/FormStatus/id/<--{$vo.id}-->/status/<--{$vo.is_pass}-->">&nbsp;<i class="fa fa-edit"></i></a>
                                                <{elseif condition="$vo['is_pass'] == 1" /}>
                                                    <font color="#3cf040">申请通过</font>
                                                    <a href="__ROOT__/Cms/InnoCustomer/FormStatus/id/<--{$vo.id}-->/status/<--{$vo.is_pass}-->">&nbsp;<i class="fa fa-edit"></i></a>
                                                <{else /}>
                                                    <font color="red">申请失败</font>
                                                    <a href="__ROOT__/Cms/InnoCustomer/FormStatus/id/<--{$vo.id}-->/status/<--{$vo.is_pass}-->">&nbsp;<i class="fa fa-edit"></i></a>
                                                <{/if}>
                                            </td>
                                            <td>
                                                <{if condition="$vo['is_contact'] eq '0'"}>
                                                    <font color="#3cb1f0">未联系</font>
                                                    <a href="__ROOT__/Cms/InnoCustomer/FormContact/id/<--{$vo.id}-->/contact/<--{$vo.is_contact}-->">&nbsp;<i class="fa fa-edit"></i></a>
                                                <{else /}>
                                                    <font color="#3cf040">已联系</font>
                                                    <a href="__ROOT__/Cms/InnoCustomer/FormContact/id/<--{$vo.id}-->/contact/<--{$vo.is_contact}-->">&nbsp;<i class="fa fa-edit"></i></a>
                                                <{/if}>
                                            </td>
                                            <td><--{$vo.add_time|date='Y-m-d h:i:s',###}--></td>
                                            <td>
                                                <!-- <a href="__ROOT__/Cms/InnoCustomer/editForm/id/"><button id="bj">编辑</button></a>&nbsp; -->
                                                <a href="__ROOT__/Cms/InnoCustomer/delData/id/<--{$vo.id}-->"><button id="sc">删除</button></a>&nbsp;
                                                <!-- <a href="#" target="_top"><button id="tg">通过/未通过</button></a>&nbsp; -->
                                            </td>
                                        </tr>
                                        <{/foreach}>

                                        <!-- <tr class="gradeC">
                                            <td>Trident</td>
                                            <td>Internet Explorer 5.0
                                            </td>
                                            <td>Win 95+</td>
                                            <td class="center">5</td>
                                            <td class="center">C</td>
                                        </tr> -->

                                    </tbody>
                                </table>

                            </div>
                        </div>
                    </div>
                </div>
                <!--PHP分页处理-->
                 <--{$page}-->

            <!-- 底部信息 -->
            <{include file="./tpl/Cms/Public/footer_us.html" /}>
        </div>
    </div>


    </div>
    <{include file="./tpl/Cms/Public/footer.html" /}>
    <!-- Data Tables -->
    <script src="__ADMIN_JS__/plugins/dataTables/jquery.dataTables.js"></script>
    <script src="__ADMIN_JS__/plugins/dataTables/dataTables.bootstrap.js"></script>

    <!-- Page-Level Scripts -->
    <script>
        $(document).ready(function () {
            $('.dataTables-example').dataTable();

            /* Init DataTables */
            var oTable = $('#editable').dataTable();

            /* Apply the jEditable handlers to the table */
            oTable.$('td').editable('../example_ajax.php', {
                "callback": function (sValue, y) {
                    var aPos = oTable.fnGetPosition(this);
                    oTable.fnUpdate(sValue, aPos[0], aPos[1]);
                },
                "submitdata": function (value, settings) {
                    return {
                        "row_id": this.parentNode.getAttribute('id'),
                        "column": oTable.fnGetPosition(this)[2]
                    };
                },

                "width": "90%",
                "height": "100%"
            });


        });

        function fnClickAddRow() {
            $('#editable').dataTable().fnAddData([
                "Custom row",
                "New row",
                "New row",
                "New row",
                "New row"]);

        }
    </script>

    <script type="text/javascript" src="http://tajs.qq.com/stats?sId=9051096" charset="UTF-8"></script><!--统计代码，可删除-->

</body>

</html>
