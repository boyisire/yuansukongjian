.nav-search {
    /*background: #2f889a;*/
    float: right;
    /*color: #fff;*/
    /*line-height: 28px;*/
    margin: 22px 10px 0 10px;
    /*padding: 0 8px 2px;*/
    cursor: pointer;
    border-radius: 3px;
    /*border: 1px solid #2f889a;*/
    box-shadow: 0 1px 1px rgba(0, 0, 0, 0.04);
}

.fa {
    display: block;
    font: normal normal normal 14px/1 FontAwesome;
    font-size: inherit;
    text-rendering: auto;
}

.fa-fontsize {
    display: inline-block;
    font: normal normal normal 22px/1 FontAwesome;
    text-rendering: auto;
    -webkit-font-smoothing: antialiased;
}

.logo-site, .logo-sites {
    position: relative;
    float: left;
    margin: 10px 0 0 10px;
    width: 220px;
    max-height: 55px;
    overflow: hidden;
    transition-duration: .5s;
}

.logo-site img, .logo-sites img {
    width: 220px;
    max-height: 50px;
}

.mobile-login a {
    float: right;
    color: #fff;
    line-height: 25px;
    margin: 24px 10px 0 10px;
    padding: 0 8px;
    cursor: pointer;
}

.logo-site::before {
    content: "";
    position: absolute;
    left: -665px;
    top: -460px;
    width: 220px;
    height: 15px;
    background-color: rgba(255, 255, 255, .5);
    -webkit-transform: rotate(-45deg);
    -moz-transform: rotate(-45deg);
    -ms-transform: rotate(-45deg);
    -o-transform: rotate(-45deg);
    transform: rotate(-45deg);
    -webkit-animation: searchLights 1s ease-in 1s infinite;
    -o-animation: searchLights 1s ease-in 1s infinite;
    animation: searchLights 1s ease-in 1s infinite;
}

.site-title {
    font-size: 24px;
    font-size: 2.4rem;
    font-weight: 700;
    padding: 4px 0 2px 0;
}

#site-nav-wrap {
    float: right;
    max-width: 70%;
}

#sidr-close {
    display: none;
}

.nav-menu, .nav-menu * {
    margin: 0;
    padding: 0;
    list-style: none;
    _float: left;
}
#navigation-toggle, .sidr, #sidr-close, #mobile-search {
    display: none;
}
#site-nav .down-menu li {
    display: block;
    float: left;
    height: 45px;
    line-height: 45px;
}

.nav-menu li {
    position: relative;
    white-space: nowrap;
    *white-space: normal;
    -webkit-transition: background .2s;
    transition: background .2s;
}

#navigation-toggle {
    color: #999;
    line-height: 25px;
    padding: 4px 8px;
}

.nav-menu > li {
    float: left;
}

#site-nav .down-menu a {
    padding: 0 13px;
    color: #444;
    text-align: left;
    transition-duration: .8s;
}

.nav-menu a {
    display: block;
    position: relative;
    zoom: 1;
}

.main-nav {
    float: right;
    margin: 15px 0 0 0;
}

.nav-menu {
    float: left;
}


/*动画*/
@-webkit-keyframes searchLights {
    0% {
        left: -100px;
        top: 0;
    }

    to {
        left: 120px;
        top: 100px;
    }
}

@-o-keyframes searchLights {
    0% {
        left: -100px;
        top: 0;
    }

    to {
        left: 120px;
        top: 100px;
    }
}

@-moz-keyframes searchLights {
    0% {
        left: -100px;
        top: 0;
    }

    to {
        left: 120px;
        top: 100px;
    }
}

@keyframes searchLights {
    0% {
        left: -100px;
        top: 0;
    }

    to {
        left: 120px;
        top: 100px;
    }
}


    /*完毕*/

@media screen and (max-width: 1210px) {
    #top-menu {
        width: 98%;
    }

    .top-nav {
        width: 98%;
    }
}

/*------------------搜索--------------------*/

#search-main {
    position: fixed;
    left: 0;
    right: 0;
    background: #fff;
    width: 1210px;
    text-align: center;
    margin: 7px auto 5px;
    padding: 10px 20px 20px 20px;
    display: none;
    z-index: 999;
    border: 1px solid #ddd;
    border-radius: 2px;
    box-shadow: 0 0 4px rgba(0, 0, 0, 0.3);
}
#searchbar {
    width: 90%;
    margin: 10px auto 0;
}
#searchform input {
    float: left;
    width: 75%;
    height: 37px;
    line-height: 37px;
    font: 14px "Microsoft YaHei", Helvetica;
    padding: 2px 10px;
    background: #ebebeb;
    border: 1px solid #ebebeb;
    border-radius: 2px 0 0 2px;
    -webkit-appearance: none;
}
#searchform button {
    overflow: visible;
    position: relative;
    border: 0;
    cursor: pointer;
    height: 37px;
    width: 25%;
    color: #fff;
    text-transform: uppercase;
    background: #2f889a;
    border-radius: 0 2px 2px 0;
}