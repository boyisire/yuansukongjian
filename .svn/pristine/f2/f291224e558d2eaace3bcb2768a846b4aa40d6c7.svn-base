        }
    });
    //清空删除文件
    $("#uploadList_"+id).removeClass("upload_append_list").html('');
}


//AJAX表单提交
function submitForm()
{
    var name = $('#name'),
        regex = /^[1][3|4|5|7|8][\d]{9}$/;
    var file_form = $("[tag='RoomForm']");
    //var show_img = $("[tag='show_photo_upload_img']");
    var acceptUrl= $("#acceptUrl").val();
    var options = {
        type : 'post',
        url : acceptUrl,
        dataType: 'json',
        contentType: "application/json; charset=utf-8",
        beforeSubmit:function(){
        //提交前,在此处可以做表单验证
            //校验空间Logo
            /*var x = $("#logo").val();
            if (x == '') {
                $(".te-lg-warn").attr('style',"color:red");
                return false;
            } else {
                $(".te-lg-warn").attr('style',"");
            }*/

            //校验空间名称
            var x = $('#name').val();
            if (x == '') {
                $('#right1').hide();
                $('#error1').show();
                return false;
            } else if (x.length < 2) {
                $('#right1').hide();
                $('#error1').show();
                return false;
            } else {
                $('#error1').hide();
                $('#right1').show();
            }

            //校验空间描述
            var x = $('#presentation').val();
            if (x == '') {
                $('#right2').hide();
                $('#error2').show();
                return false;
            } else if (x.length < 50) {
                $('#right2').hide();
                $('#error2').show();
                return false;
            } else {
                $('#error2').hide();
                $('#right2').show();
            }

            //校验地址
            var x = $('#address').val();
            if (x == '') {
                $('#right3').hide();
                $('#error3').show();
                return false;
            } else if (x.length < 6) {
                $('#right3').hide();
                $('#error3').show();
                return false;
            } else {
                $('#error3').hide();
                $('#right3').show();
            }

            //负责人
            var x = $('#principal').val();
            if (x == '') {
                $('#right4').hide();
                $('#error4').show();
                return false;
            } else if (x.length < 2) {
                $('#right4').hide();
                $('#error4').show();
                return false;
            } else {
                $('#error4').hide();
                $('#right4').show();
            }

            //手机号
            var x = $('#phone').val();
            if (x == '') {
                $('#right5').hide();
                $('#error5').show();
                return false;
            } else if (!regex.test(x)) {
                $('#right5').hide();
                $('#error5').show();
                return false;
            } else {
                $('#error5').hide();
                $('#right5').show();
            }


            //校验工位金额
            var y = $('#station').val();
            if (y == '') {
                $('#right6').hide();
                $('#error6').show();
            } else {
                $('#right6').show();
                $('#error6').hide();
            }

            //招租价格校验
            var y = $('#square').val();
            var z = $('#station').val();
            if (y == '' || z == '') {
                $('#right6').hide();
                $('#error6').show();
            } else {
                $('#right6').show();
                $('#error6').hide();
            }

        },
        success:function(e) {
            var eCode= e.code;
            var eMsg = e.msg;
            var eData= e.data;
            if(eCode != 0){
                if(eData == 0){
                	//alert(eMsg);
                }else{
                    alert(eCode+':'+eMsg+':'+eData);
                }
            }else{
                //alert(e.data);
                //空间轮播图片上传
                $("#uploadForm").attr('action',url_add);
                if (ZYFILE.funReturnNeedFiles().length > 0) {
                    //alert('开始上传空间图片');
                    $("#fileSubmit").click();
                }
            }
            if(ZYFILE.funReturnNeedFiles().length > 0){

            }else{
           	 	location.href="<?php echo U('home/form/message');?>";
            }
        },
        error:function(XmlHttpRequest, textStatus, errorThrown){
            //alert(textStatus);
            alert(errorThrown);
        }
    };
    //表单提交
    file_form.ajaxSubmit(options);
}



